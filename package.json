    "@nestjs/terminus": "^11.0.0",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "^11.0.0",
    "@nestjs/websockets": "^11.1.9",
    "@scalar/nestjs-api-reference": "^1.0.7",
    "@sentry/bun": "^10.26.0",
    "@sentry/cli": "^2.58.2",
    "@sentry/nestjs": "^10.26.0",
    "@sentry/profiling-node": "^10.26.0",
    "argon2": "^0.44.0",
    "aws-sdk-v3-nest": "^1.0.1",
    "axios": "^1.13.2",
