  "scripts": {
    "build": "nest build",
    "build:digitalocean": "yarn install --production=false && yarn run build && rm -rf node_modules && yarn install --production --frozen-lockfile && yarn run sentry:sourcemaps",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug 0.0.0.0:9229 --watch",
    "start:prod": "node dist/main",
    "lint": "eslint src/ --fix",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -d src/modules/database/database.datasource.ts",
    "migration:create": "cross-var ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli migration:create ./src/migrations/$npm_config_name",
    "migration:generate": "yarn run typeorm -- migration:generate src/migrations/migration",
    "migration:run": "yarn run build && yarn run typeorm -- migration:run",
    "migration:revert": "yarn run typeorm -- migration:revert",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org codaft-oi --project storage-backend ./dist && sentry-cli sourcemaps upload --org codaft-oi --project storage-backend ./dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:e2e:watch": "jest --config ./test/jest-e2e.json --watch",
    "test:e2e:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --config ./test/jest-e2e.json",
    "test:unit": "jest --testPathPattern=src --testPathIgnorePatterns=src/modules/.*\\.e2e-spec\\.ts$",
    "test:integration": "jest --testPathPattern=src --testPathIgnorePatterns=src/modules/.*\\.spec\\.ts$ --testPathPattern=.*\\.e2e-spec\\.ts$",
    "test:ci": "jest --ci --coverage --watchAll=false --verbose"
  },
